								"exec": [
									"var name = \"Mild Rain\";",
									"var conditions = {",
									"    \"type\": \"OPERATOR\",",
									"    \"content\": {",
									"        \"operator\": \"AND\"",
									"    },",
									"    \"children\": [{",
									"            \"type\": \"OPERATOR\",",
									"            \"content\": {",
									"                \"operator\": \"EQUAL\"",
									"            },",
									"            \"children\": [{",
									"                    \"type\": \"PARAMETER\",",
									"                    \"content\": {",
									"                        \"parameter\": \"precipitationType\"",
									"                    }",
									"                },",
									"                {",
									"                    \"type\": \"CONST\",",
									"                    \"content\": {",
									"                        \"const\": \"rain\"",
									"                    }",
									"                }",
									"            ]",
									"        },",
									"        {",
									"            \"type\": \"OPERATOR\",",
									"            \"content\": {",
									"                \"operator\": \"GREATER\"",
									"            },",
